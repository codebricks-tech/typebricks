{
    "name": "dddtsframework",
    "private": true,
    "version": "0.0.72",
    "description": "",
    "main": "index.js",
    "types": "index.d.ts",
    "files": [
        "/"
    ],
    "scripts": {
        "prebuild": "rm -Rf ./dist",
        "build": "tsc --project tsconfig.build.json",
        "postbuild": "npm run distPackage",
        "distPackage": "cp package.json ./dist/ && npx json -I -f ./dist/package.json -e 'this.scripts={}' -e 'this.private=false' ",
        "testAndMigrate": "npm run migration && mocha -r ts-node/register tests/**/*.test.ts -g 'bank account events'",
        "test": "mocha --full-trace -r ts-node/register tests/domain/**/*.test.ts",
        "testExample": "export $(cat .env | xargs) && mocha -r ts-node/register examples/**/**/**/BankAccountOverviewProjection.test.ts",
        "coverage": "nyc -r text -e .ts -x \"tests/*.test.ts\" npm run test",
        "migration": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/infrastructure/app-data-source.ts"
    },
    "dependencies": {
        "@aws-sdk/client-eventbridge": "^3.222.0",
        "is-what": "^4.1.15",
        "node-gzip": "^1.1.2",
        "pg": "^8.11.3",
        "shallow-equal": "^3.1.0",
        "shallow-equal-object": "^1.1.1",
        "typeorm": "^0.3.6",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.97",
        "@types/chai": "^4.3.0",
        "@types/chai-as-promised": "^7.1.5",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.31",
        "@types/node-gzip": "^1.1.3",
        "@types/uuid": "^8.3.4",
        "aws-sdk-client-mock": "^0.6.2",
        "chai": "^4.3.6",
        "dotenv": "^16.0.0",
        "mocha": "^9.2.2",
        "nyc": "^15.1.0",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3"
    }
}
